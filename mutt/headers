# Headers
ignore      *

unignore    From:
unignore    To:
unignore    Cc:
unignore    Bcc:
unignore    Subject:
unignore    Date:
unignore    Organization:
unignore    Mail-followup-to:
#unignore    X-Mailer:
#unignore    X-Url:
#unignore    X-Mailer:
#unignore    User-Agent:
#unignore    Message-ID:

unhdr_order *
hdr_order   From: To: Cc: Bcc: Subject: Date: Organization:

set hdrs    # include `my_hdr' lines in outgoing messages
