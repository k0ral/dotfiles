// Place your key bindings in this file to overwrite the defaults
[ { "key": "ctrl+t", "command": "workbench.action.quickOpen" }
, { "key": "ctrl+up", "command": "spaceBlockJumper.moveUp", "when": "textInputFocus" }
, { "key": "ctrl+down", "command": "spaceBlockJumper.moveDown", "when": "textInputFocus" }
, { "key": "alt+delete", "command": "editor.action.clipboardCutAction", "when": "textInputFocus && !editorReadonly" }
, { "key": "alt+left","command": "cursorHome", "when": "textInputFocus" }
, { "key": "alt+right","command": "cursorEnd", "when": "textInputFocus" }
, { "key": "alt+c", "command": "editor.action.commentLine", "when": "editorTextFocus && !editorReadonly" }
]